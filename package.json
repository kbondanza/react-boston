{
  "name": "react-boston",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "gh-pages": "^1.0.0",
    "grid-styled": "^3.2.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-ga": "^2.2.0",
    "react-google-maps": "^9.4.5",
    "react-icons": "^2.2.5",
    "react-router": "^4.1.1",
    "react-scroll": "^1.5.2",
    "react-tap-event-plugin": "^3.0.2",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "react-scripts": "1.1.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p start-js",
    "build": "npm run react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "homepage": "http://www.reactboston.com"
}
